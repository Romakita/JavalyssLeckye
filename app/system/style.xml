<global author="Lenzotti Romain" name="default" version="1.0">
<!-- CDATA[[ 
 
 Global Font
 Style de la police par défaut de la bibliothèque Window
 
]]-->
	<font>
		<family>Arial, Helvetica, sans-serif</family>
		<size>12px</size>
	</font>
	<color>#000</color>
<!-- CDATA[[ 
 
 Global Border
 Style des bordures de fenêtre à utiliser par défaut.
 
]]-->
	<border>
		<width>1px</width>
		<style>solid</style>
		<color>#B2B1B1</color>
		<radius>5px</radius>
	</border>
<!-- CDATA[[ 
 
 Global Background
 Couleur de fond des fenêtres.
 
]]-->
	<background>
		<color>#EBEBEB</color>
	</background>
<!-- CDATA[[ 
 
 Global Box Shadow
 Style des ombres des éléments.
 
]]-->
	<boxShadow>0px 0px 8px #000</boxShadow>
<!-- CDATA[[ 
 
 Global Header
 Style des entêtes de fenêtre de Window.
 
]]-->
	<header>
		<color>#333</color>
		<text>text-shadow: 0px 1px white;</text>
		<background>
			<color>#FBFCFC</color>
			<image>system/images/windowjs/header-bg.png</image>
			<linearGradient> 
				background-image: -ms-linear-gradient(top, #FBFCFC 0%, #EBEBEB 99%);
				background-image: -moz-linear-gradient(top, #FBFCFC 0%, #EBEBEB 99%);
				background-image: -o-linear-gradient(top, #FBFCFC 0%, #EBEBEB 99%);
				background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #FBFCFC), color-stop(1, #EBEBEB));
				background-image: -webkit-linear-gradient(top, #FBFCFC 0%, #EBEBEB 99%);
				background-image: linear-gradient(top, #FBFCFC 0%, #EBEBEB 99%);	
			</linearGradient>
			<position>bottom</position>
			<!--<repeat>repeat-x</repeat>-->
		</background>
		
		<border>
			<color>#B2B1B1</color>
		</border>
			
	</header>
<!-- CDATA[[ 
 
 Global Header
 Style des entêtes de fenêtre de Window.
 
]]-->	
	<footer>
		<color>white</color>
		<text>text-shadow:0 -1px 0 rgba(0, 0, 0, 0.3);</text>
		<background>
			<color>#EBEBEB</color>
			<image>window/images/footer-bg.png</image>
			<position>top</position>
			<repeat>repeat-x</repeat>
			<linearGradient>
				background-image: -ms-linear-gradient(top, #EBEBEB 0%, #D7D7D7 99%); 
				background-image: -moz-linear-gradient(top, #EBEBEB 0%, #D7D7D7 99%); 
				background-image: -o-linear-gradient(top, #EBEBEB 0%, #D7D7D7 99%); 
				background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #EBEBEB), color-stop(1, #D7D7D7)); 
				background-image: -webkit-linear-gradient(top, #EBEBEB 0%, #D7D7D7 99%);
				background-image: linear-gradient(top, #EBEBEB 0%, #D7D7D7 99%);
			</linearGradient>
		</background>
		<border>
			<color>#B2B1B1</color>
		</border>
		
	</footer>
<!-- CDATA[[ 
 
 Global Body
 Couleur de fond des éléments de type Body. (Corps des Fenêtres Window)
 
]]-->
	<body>
		<background>
			<color>#FFF</color>
		</background>
	</body>
<!-- CDATA[[ 
 
 Global Button
 Style générale des éléments de type Bouton.
 
]]-->
	<button>
<!-- CDATA[[ 
 
 Button Padding
 Définissez ici la marge interne à appliquer aux boutons.
 
]]-->
		<padding>
			<left>5px</left>
			<top>2px</top>
			<right>5px</right>
			<bottom>2px</bottom>
		</padding>
<!-- CDATA[[ 
 
 Button Normal
 Définissez ici le style du bouton à l'état normal.
 
]]-->
		<normal>
			<!-- Couleur du texte -->
			<color>#333</color>
			<text>text-shadow:0 -1px 0 rgb(255, 255, 255);</text>
			<background>
				<color>#D7D7D7</color>
				<image>system/images/windowjs/button-bg.png</image>
				<linearGradient>
					background-image: -ms-linear-gradient(top, #E4E4E4 0%, #D7D7D7 99%);
					background-image: -moz-linear-gradient(top, #E4E4E4 0%, #D7D7D7 99%); 
					background-image: -o-linear-gradient(top, #E4E4E4 0%, #D7D7D7 100%);
					background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #E4E4E4), color-stop(1, #D7D7D7));
					background-image: -webkit-linear-gradient(top, #E4E4E4 0%, #D7D7D7 99%);
					background-image: linear-gradient(top, #E4E4E4 0%, #D7D7D7 99%);
					filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#E4E4E4, endColorstr=#D7D7D7);
    				-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#E4E4E4, endColorstr=#D7D7D7)";
				</linearGradient>
				<repeat>repeat-x</repeat>
				<position>top</position>
			</background>
			<!-- Bordure arrondie -->
			<border>
				<style>solid</style>
				<width>1px</width>
				<radius>3px</radius>
				<alternate>#F9F9F9</alternate>
			</border>
		</normal>
<!-- CDATA[[ 
 
 Button Over
 Définissez ici le style du bouton à l'état survolé.
 
]]-->
		<over>
			<!-- Couleur du texte -->
			<color>white</color>
			<text>text-shadow:0 -1px 0 rgba(0, 0, 0, 0.3);</text>
			<background>
				<color>#9f272d</color>
				<image>window/images/win-btn-hover.png</image>
				<linearGradient>
					background-image: -ms-linear-gradient(top, #B03E3C 0%, #9f272d 99%);
					background-image: -moz-linear-gradient(top, #B03E3C 0%, #9f272d 99%);
					background-image: -o-linear-gradient(top, #B03E3C 0%, #9f272d 99%);
					background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #B03E3C), color-stop(1, #9f272d));
					background-image: -webkit-linear-gradient(top, #B03E3C 0%, #9f272d 99%);
					background-image: linear-gradient(top, #B03E3C 0%, #9f272d 99%);
					filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#B03E3C, endColorstr=#9f272d);
    				-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#B03E3C, endColorstr=#9f272d)";
				</linearGradient>
				<repeat>repeat-x</repeat>
				<position>top</position>
			</background>
			
			<border>
				<style>solid</style>
				<width>1px</width>
				<color>#660000</color>
				<alternate>#c19494</alternate>
			</border>
		</over>
<!-- CDATA[[ 
 
 Button Selected
 Définissez ici le style du bouton à l'état selectionné. Par défaut l'etat séléctionné reprend le style
 de l'état survolé.
 
]]-->
		<selected>
			<!-- Couleur du texte -->
			<color>#666</color>
			<text>text-shadow:none;</text>
			<background>
				<color>white</color>
				<image>none</image>
				<repeat>repeat-x</repeat>
				<position>top</position>
			</background>
			
			<border>
				<style>solid</style>
				<width>1px</width>
				<color>#B2B1B1</color>
			</border>	
		</selected>
<!-- CDATA[[ 
 
 Button Submit
 Définissez ici le style du bouton de type submit. Par défaut l'etat séléctionné reprend le style
 de l'état normal.
 
]]-->		
		<submit>
			<!-- Couleur du texte -->
			<color>white</color>
			<text>text-shadow:0 -1px 0 rgba(0, 0, 0, 0.3);</text>
			<background>
				<color>#206c90</color>
				<image>window/images/win-btn-submit.png</image>
				<linearGradient>
					background-image: -ms-linear-gradient(top, #298CBA 0%, #206C90 99%);
					background-image: -moz-linear-gradient(top, #298CBA 0%, #206C90 99%);
					background-image: -o-linear-gradient(top, #298CBA 0%, #206C90 99%);
					background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #298CBA), color-stop(1, #206C90));
					background-image: -webkit-linear-gradient(top, #298CBA 0%, #206C90 99%);
					background-image: linear-gradient(top, #298CBA 0%, #206C90 99%);
					filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#298CBA, endColorstr=#206C90);
    				-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#298CBA, endColorstr=#206C90)";
				</linearGradient>
				<repeat>repeat-x</repeat>
				<position>top</position>
			</background>
			
			<border>
				<style>solid</style>
				<width>1px</width>
				<color>#006d9c</color>
			</border>
		</submit>
<!-- CDATA[[ 
 
